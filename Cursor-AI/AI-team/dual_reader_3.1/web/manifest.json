    {
      "name": "Library",
      "short_name": "Library",
      "description": "Open your book library",
      "url": "/",
      "icons": [
        {
          "src": "icons/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Continue Reading",
      "short_name": "Continue",
      "description": "Continue reading your last book",
      "url": "/reader",
      "icons": [
        {
          "src": "icons/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "book",
          "accept": [".epub", ".mobi", "application/epub+zip", "application/x-mobipocket-ebook"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+epub",
      "url": "/?file=%s"
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "startup_url": "/"
}
